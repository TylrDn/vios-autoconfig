# HMC hostname or IP address
HMC_HOST=hmchost.example

# HMC user with privileges to run VIOS commands
HMC_USER=hmcuser

# Managed system identifier
MS=Server-9119-XXX-SN12345

# Primary and secondary VIOS logical partition names
VIOS1=VIOS1
VIOS2=VIOS2

# Network configuration for SEA
BACKING_ETH=ent2
VSWITCH=VSWITCH0
SEA_VLAN=1
TRUNK_ADAPTER=ent10

# Extra SSH options passed to the HMC CLI (optional)
SSH_OPTS="-o BatchMode=yes -o StrictHostKeyChecking=accept-new"
